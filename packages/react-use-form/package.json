    "test": "node --experimental-vm-modules ./node_modules/.bin/jest"
  },
  "dependencies": {
    "@pinglue/utils": "^0.0.0",
    "object-path-immutable": "^4.1.2",
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@types/react": "17.0.21",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "eslint": "^7.31.0",
    "jest": "^27.0.6",
    "typedoc": "^0.21.4",
    "typescript": "4.3.5"
  }
}
