{
  "name": "@pgweb/react-use-form",
  "version": "0.0.0",
  "type": "module",
  "main": "./lib/index.js",
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "watch": "./node_modules/.bin/tsc -w",
    "build-ts": "./node_modules/.bin/tsc -b",
